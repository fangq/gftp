msgstr "%pu = прокси потребител"

#: lib/config_file.c:103
#, c-format
msgid "%hu = host user"
msgstr "%hu = хост потребител"

#: lib/config_file.c:104
#, c-format
msgid "%pp = proxy pass"
msgstr "%pp = прокси парола"

#: lib/config_file.c:105
#, c-format
